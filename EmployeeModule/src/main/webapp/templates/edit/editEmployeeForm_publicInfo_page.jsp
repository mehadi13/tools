
<div class="row">
	<div class="col-md-6 col-sm-6">
		<h3>Contact Informaton</h3>

		<div class="form-group row">
			<label for="workingAddress" class="col-sm-4 control-label">Working
				Address</label>

			<div class="col-sm-8">
				<select class="form-control" name="workingAddress"
					id="workingAddress" required>
					
				</select>
			</div>
		</div>

		<div class="form-group row">
			<label for="workMobile" class="col-sm-4 control-label">Work
				Mobile</label>
			<div class="col-sm-8">
				<input type="text" value="" class="form-control" name="workMobile" required id="workMoble">
			</div>
		</div>

		<div class="form-group row">
			<label for="workLocation" class="col-sm-4 control-label">Work
				Location</label>
			<div class="col-sm-8">
				<input type="text" value="" class="form-control" name="workLocation" required id="workLocaion">
			</div>
		</div>

		<div class="form-group row">
			<label for="workEmail" class="col-sm-4 control-label">Work
				Email</label>
			<div class="col-sm-8">
				<input type="email" value="" class="form-control" name="workEmail" required id="workEmail">
			</div>
		</div>

		<div class="form-group row">
			<label for="workPhone" class="col-sm-4 control-label">Work
				Phone</label>
			<div class="col-sm-8">
				<input type="text" value="" class="form-control" name="workPhone" id="workPhone">
			</div>
		</div>
	</div>
	<!-- end of contact information col -->


	<div class="col-md-6 col-sm-6">
		<h3>Position</h3>

		<div class="form-group row">
			<label for="department" class="col-sm-4 control-label">Department</label>
			<div class="col-sm-8">
				<select class="form-control" name="department" id="department"
					required>
				</select>
			</div>
		</div>

		<div class="form-group row">
			<label for="jobTitle" class="col-sm-4 control-label">Job
				title</label>
			<div class="col-sm-8">
				<select class="form-control" name="jobTitle" id="jobTitle" required>
				</select>
			</div>
		</div>

		<div class="form-group row">
			<label for="manager" class="col-sm-4 control-label">Manager</label>
			<div class="col-sm-8">
				<select class="form-control" name="manager" id="manager">
				</select>
			</div>
		</div>

		<div class="form-group row">
			<label for="coach" class="col-sm-4 control-label">Coach</label>
			<div class="col-sm-8">
				<select class="form-control" name="coach" id="coach">

				</select>
			</div>
		</div>

	</div>
</div>
<!-- end of row -->

<div class="form-group row">
	<div class="col-sm-8 mywidth_full">
		<textarea type="text" value="" class="form-control" name="otherInformation"
			placeholder="Other Information.." id="otherInformation"></textarea>
	</div>
</div>

