
<div class="row">
	<div class="col-md-6 col-sm-6">

		<h3>Citizenship & Other Information</h3>
		<div class="form-group row">
			<label for="nationality" class="col-sm-4 control-label">Nationality</label>
			<div class="col-sm-8">
				<select class="form-control" name="nationality" id="nationality">
				</select>
			</div>
		</div>

		<div class="form-group row">
			<label for="identificationNo" class="col-sm-4 control-label">Identification
				No</label>
			<div class="col-sm-8">
				<input type="text" value="" class="form-control" name="identificationNo"
					placeholder="Identification No" id="identificationNo">
			</div>
		</div>
		<div class="form-group row">
			<label for="passportNo" class="col-sm-4 control-label">Passport
				No</label>
			<div class="col-sm-8">
				<input type="text" value="" class="form-control" name="passportNo"
					placeholder="Passport No" id="passportNo">
			</div>
		</div>
		<div class="form-group row">
			<label for="bankAccountNumber" class="col-sm-4 control-label">Bank
				Account Number</label>
			<div class="col-sm-8">
				<input type="text" value="" class="form-control" name="bankAccountNumber"
					placeholder="Bank Account Number" id="bankAccountNumber">
			</div>
		</div>

		<h3>Status</h3>
		<div class="form-group row">
			<label for="gender" class="col-sm-4 control-label">Gender</label>
			<div class="col-sm-8">
				<select class="form-control" name="gender" id="gender" required>
					
				</select>
			</div>
		</div>
		<div class="form-group row">
			<label for="maritalStatus" class="col-sm-4 control-label">Marital
				Status</label>
			<div class="col-sm-8">
				<select class="form-control" name="maritalStatus" id="maritalStatus">
					
				</select>
			</div>
		</div>
	</div>
	<!-- end of contact information col -->


	<div class="col-md-6 col-sm-6">

		<h3>Contact Information</h3>
		<div class="form-group row">
			<label for="homeAddress" class="col-sm-4 control-label">Home
				Address</label>
			<div class="col-sm-8">
				<input type="text" value="" class="form-control" name="homeAddress"
					placeholder="Home Address" id="homeAddress">
			</div>
		</div>

		<h3>Birth</h3>
		<div class="form-group row">
			<label for="dateOfBirth" class="col-sm-4 control-label">Date
				of Birth</label>
			<div class="col-sm-8">
				<input type="date" value="" class="form-control" name="dateOfBirth"
					placeholder="Date of Birth" required id="dateOfBirth">
			</div>
		</div>

	</div>

</div>
<!-- end of row -->

