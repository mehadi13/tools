
<div class="row">
	<div class="col-md-6 col-sm-6">

		<h3>Citizenship & Other Information</h3>
		<div class="form-group row">
			<label class="col-sm-4 control-label">Nationality</label>
			<div class="col-sm-8">
				<label id="nationality" class="control-label text-info"></label>
			</div>
		</div>

		<div class="form-group row">
			<label class="col-sm-4 control-label">Identification No</label>
			<div class="col-sm-8">
				<label id="identificationNo" class="control-label text-info"></label>
			</div>
		</div>
		<div class="form-group row">
			<label class="col-sm-4 control-label">Passport No</label>
			<div class="col-sm-8">
				<label id="passportNo" class="control-label text-info"></label>
			</div>
		</div>
		<div class="form-group row">
			<label class="col-sm-4 control-label">Bank Account Number</label>
			<div class="col-sm-8">
				<label id="bankAccountNumber" class="control-label text-info"></label>
			</div>
		</div>

		<h3>Status</h3>
		<div class="form-group row">
			<label class="col-sm-4 control-label">Gender</label>
			<div class="col-sm-8">
				<label id="gender" class="control-label text-info"></label>
			</div>
		</div>
		<div class="form-group row">
			<label class="col-sm-4 control-label">Marital Status</label>
			<div class="col-sm-8">
				<label id="maritalStatus" class="control-label text-info"></label>
			</div>
		</div>
	</div>
	<!-- end of contact information col -->


	<div class="col-md-6 col-sm-6">

		<h3>Contact Information</h3>
		<div class="form-group row">
			<label class="col-sm-4 control-label">Home Address</label>
			<div class="col-sm-8">
				<label id="homeAddress" class="control-label text-info"></label>
			</div>
		</div>

		<h3>Birth</h3>
		<div class="form-group row">
			<label class="col-sm-4 control-label">Date of Birth</label>
			<div class="col-sm-8">
				<label id="dateOfBirth" class="control-label text-info"></label>
			</div>
		</div>

	</div>

</div>
<!-- end of row -->

